body .ui-datepicker{z-index:9999 !important;padding:15px 20px;width:285px;box-sizing:border-box}.ui-datepicker-prev,.ui-datepicker-next{cursor:pointer}.ui-datepicker-month,.ui-datepicker-month option{text-transform:capitalize}input[type=text].hasDatepicker{background-image:url(//www.hithyoga.com/wp-content/plugins/wp-datepicker/css/../img/calendar.svg);background-repeat:no-repeat;background-position:99% center;background-size:auto 60%}